{
  "name": "easyquiz",
  "version": "1.0.0",
  "description": "Quiz management application",
  "productName": "EasyQuiz",
  "author": "romesh-jaya <monty.rom.jay@gmail.com>",
  "private": true,
  "scripts": {
    "start": "quasar dev",
    "build": "cross-env ENV=staging quasar build",
    "deploy": "vercel",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "check-types": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "firebase": "^9.10.0",
    "humanize-duration": "^3.27.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "sortablejs": "^1.15.0",
    "sortablejs-vue3": "^1.2.3",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue-snip": "^2.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^12.20.21",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vue-tsc": "^0.40.13"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
